
public class Caneta {

	public String Modelo;
	private float Ponta;
	
	
	public String getModelo() {
		return this.Modelo;
	}
	
	public void setModelo(String m) {
		this.Modelo = this.m;
	}
	public float getPonta() {
		return Ponta;
	}
	public float setPonta(p) {
		this.Ponta = p;
		return p;
	}
	
	
}
