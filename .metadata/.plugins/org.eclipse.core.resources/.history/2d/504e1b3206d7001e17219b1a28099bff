
public class Caneta {

	public String Modelo;
	private float Ponta;
	
	
	public String getModelo() {
		return this.Modelo;
	}
	
	public void setModelo(String m) {
		this.Modelo = m;
	}
	public float getPonta() {
		return Ponta;
	}
	public void setPonta(float p) {
		this.Ponta = p;
	}
	
	public void status() {
		System.out.println("MODELO =" this.Modelo);
	}
	
	
}
